# Example secrets file for Streamlit Cloud
# 
# To use this in Streamlit Cloud:
# 1. Go to your app settings on share.streamlit.io
# 2. Click "Secrets" in the left sidebar
# 3. Paste the configuration below (uncommented) and add your model URLs
# 4. Save and the app will automatically use these URLs
#
# For local development:
# Copy this file to .streamlit/secrets.toml and uncomment the URLs

[model_urls]
# shha_url = "https://your-model-hosting-url.com/SHHA.pth"
# shhb_url = "https://your-model-hosting-url.com/SHHB.pth"

# Example with Google Drive direct links:
# shha_url = "https://drive.google.com/uc?export=download&id=YOUR_FILE_ID"
# shhb_url = "https://drive.google.com/uc?export=download&id=YOUR_FILE_ID"
